{
  "name": "country-explorer",
  "version": "1.0.0",
  "description": "A modern country explorer application with dark/light mode support",
  "private": true,
  "type": "module",
  "keywords": [
    "nuxt",
    "vue",
    "countries",
    "explorer",
    "dark-mode",
    "responsive"
  ],
  "author": "Country Explorer Team",
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build.js",
    "build:clean": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt build",
    "build:verbose": "nuxt build",
    "build:analyze": "cross-env ANALYZE=true nuxt build",
    "dev": "node scripts/dev.js",
    "dev:clean": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt dev",
    "generate": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt generate",
    "preview": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "demo": "node scripts/demo.js"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxt/ui": "^3.1.3",
    "@nuxtjs/color-mode": "^3.5.1",
    "@pinia/nuxt": "^0.11.0",
    "eslint": "^9.27.0",
    "nuxt": "^3.17.4",
    "pinia": "^3.0.2",
    "typescript": "^5.8.3",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "happy-dom": "^17.4.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
