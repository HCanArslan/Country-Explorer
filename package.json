{
  "name": "country-explorer",
  "version": "1.0.0",
  "description": "A modern country explorer application with dark/light mode support",
  "private": true,
  "type": "module",
  "keywords": [
    "nuxt",
    "vue",
    "countries",
    "explorer",
    "dark-mode",
    "responsive"
  ],
  "author": "Country Explorer Team",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "build:vercel": "cross-env NITRO_PRESET=vercel nuxt build",
    "build:local": "node scripts/build.js",
    "build:clean": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt build",
    "build:verbose": "nuxt build",
    "build:analyze": "cross-env ANALYZE=true nuxt build",
    "build:optimized": "node scripts/optimize-build.js",
    "build:production": "cross-env NODE_ENV=production node scripts/optimize-build.js",
    "dev": "node scripts/dev.js",
    "dev:clean": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt dev",
    "generate": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt generate",
    "preview": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "cross-env NODE_OPTIONS=\"--no-deprecation\" nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "demo": "node scripts/demo.js",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "performance:analyze": "npx nuxi analyze",
    "mobile:test": "node scripts/test-mobile-performance.js"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxt/ui": "^2.22.0",
    "@nuxtjs/color-mode": "^3.5.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.0",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "autoprefixer": "^10.4.21",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "eslint": "^9.27.0",
    "h3": "^1.12.0",
    "leaflet": "^1.9.4",
    "nitropack": "^2.9.7",
    "nuxt": "^3.17.4",
    "ofetch": "^1.4.1",
    "pinia": "^3.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "ufo": "^1.5.4",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "happy-dom": "^17.4.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
